{
    "manifest_version": 2,
    "name": "Borefresh",
    "version": "0.3.1",
    "description": "Refresh my tabs automatically every 111 seconds.",
    "homepage_url": "https://github.com/bflatmajor/borefresh",
    "icons": {
        "48": "icon-running.svg",
        "96": "icon-running.svg"
    },
    "browser_action": {},
    "background": {
        "scripts": ["browser-polyfill.min.js", "background.js"]
    },
    "permissions": ["tabs", "storage"]
}
